{
  "name": "fowcrawler",
  "productName": "fowcrawler",
  "version": "1.4.0",
  "description": "My Electron application description",
  "main": "src/index.js",
  "scripts": {
    "start": "electron-forge start",
    "package": "electron-forge package",
    "make": "npm run prepare-python-deps && electron-forge make",
    "make:win": "npm run verify-icons && npm run prepare-python-deps && electron-forge make --platform=win32",
    "make:win64": "npm run verify-icons && npm run prepare-python-deps && electron-forge make --platform=win32 --arch=x64",
    "make:win32": "npm run verify-icons && npm run prepare-python-deps && electron-forge make --platform=win32 --arch=ia32",
    "package:win": "npm run verify-icons && npm run prepare-python-deps && electron-forge package --platform=win32",
    "publish": "electron-forge publish",
    "lint": "echo \"No linting configured\"",
    "verify-icons": "node scripts/verify-icons.js",
    "test-python-deps": "node scripts/test-python-deps.js",
    "prepare-python-deps": "node scripts/prepare-python-deps.js",
    "dev": "concurrently \"npm run dev:vite\" \"npm run dev:electron\"",
    "dev:vite": "vite",
    "dev:electron": "wait-on http://localhost:3000 && NODE_ENV=development electron .",
    "dev:api": "cd python_api && source ../venv/bin/activate && python3 run.py",
    "dev:full": "concurrently \"npm run dev:vite\" \"npm run dev:api\" \"npm run dev:electron\"",
    "start:all": "concurrently --kill-others --success first \"npm run dev:api\" \"npm run dev:vite\"  \"wait-on http://localhost:3000 http://localhost:5002 && NODE_ENV=development electron .\"",
    "build": "vite build",
    "build:win": "npm run verify-icons && vite build && npm run make:win",
    "build:win-fallback": "vite build && electron-forge make --platform=win32 --config=./forge.config.no-icons.js",
    "make:win-fallback": "electron-forge make --platform=win32 --config=./forge.config.no-icons.js",
    "preview": "vite preview",
    "setup:python": "cd python_api && python3 -m pip install -r ../requirements.txt --user",
    "clear-cache": "rm -rf node_modules/.cache && rm -rf dist",
    "dev:fresh": "npm run clear-cache && npm run dev:full"
  },
  "keywords": [],
  "author": {
    "name": "r-thinking",
    "email": "making.r.w@gmail.com"
  },
  "license": "MIT",
  "dependencies": {
    "@types/socket.io-client": "^1.4.36",
    "@types/styled-components": "^5.1.34",
    "electron-squirrel-startup": "^1.0.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "socket.io-client": "^4.8.1",
    "styled-components": "^6.1.19"
  },
  "devDependencies": {
    "@electron-forge/cli": "^7.8.1",
    "@electron-forge/maker-dmg": "^7.8.1",
    "@electron-forge/maker-squirrel": "^7.8.1",
    "@electron-forge/maker-zip": "^7.8.1",
    "@electron-forge/plugin-auto-unpack-natives": "^7.8.1",
    "@electron-forge/plugin-fuses": "^7.8.1",
    "@electron/fuses": "^1.8.0",
    "@types/node": "^20.4.0",
    "@types/react": "^18.2.0",
    "@types/react-dom": "^18.2.0",
    "@vitejs/plugin-react": "^4.2.0",
    "concurrently": "^8.2.0",
    "electron": "36.5.0",
    "typescript": "^5.1.0",
    "vite": "^5.0.0",
    "wait-on": "^7.0.0"
  }
}
